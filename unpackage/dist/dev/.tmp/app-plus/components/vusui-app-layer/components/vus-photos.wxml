<view class="vus-layer" style="{{(setLayerStyle)}}"><view class="{{['vus-layer-photos vus-layer-anim',addAnim]}}"><block wx:if="{{options.closeBtn}}"><view class="vus-layer-close"><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="vli-error vus-layer-close-icon" bindtap="__e"></view></view></block><block wx:if="{{options.title}}"><view class="vus-layer-photos-title">{{setContent?options.content[current].title:options.title}}</view></block><block wx:if="{{options.number&&setContent}}"><view class="vus-layer-photos-number">{{current+1+" / "+options.content.length}}</view></block><view class="vus-layer-photos-content"><block wx:if="{{setContent}}"><swiper class="vus-layer-photos-swiper" current="{{current}}" data-event-opts="{{[['change',[['changeImage',['$event']]]]]}}" bindchange="__e"><block wx:for="{{options.content}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item class="vus-layer-photos-swiper-item"><image class="vus-layer-photos-image" src="{{item.src}}" mode="widthFix"></image></swiper-item></block></swiper></block><block wx:else><slot></slot></block></view></view></view>