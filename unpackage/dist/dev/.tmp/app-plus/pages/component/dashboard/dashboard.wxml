<view class="content"><view class="user-section"><image class="bg" src="/static/img/user-bg.jpg"></image><view class="user-info-box"><view class="portrait-box"><image class="portrait" src="{{userInfo.portrait||'/static/img/missing-face.png'}}" data-event-opts="{{[['tap',[['GotoLogo',['$event']]]]]}}" bindtap="__e"></image></view><view><text class="username">{{userInfo.realname||'未登录'}}</text></view><view class="list-info"><view data-event-opts="{{[['tap',[['navTo2',['/pages/sjcl/getalltasks']]]]]}}" class="list-item" bindtap="__e"><text class="data">{{datas[0]}}</text><text class="data1">台</text></view></view></view><view class="list-info-box"><view data-event-opts="{{[['tap',[['switchTo',['/pages/verification/verification']]]]]}}" class="list-item" bindtap="__e"><text class="list-num">{{datas[1]}}</text><text class="list">待检</text></view><view data-event-opts="{{[['tap',[['switchTo',['/pages/verification/verification']]]]]}}" class="list-item" bindtap="__e"><text class="list-num">{{datas[2]}}</text><text class="list">在检</text></view><view data-event-opts="{{[['tap',[['switchTo',['/pages/check/check']]]]]}}" class="list-item" bindtap="__e"><text class="list-num">{{datas[3]}}</text><text class="list">核验</text></view><view data-event-opts="{{[['tap',[['switchTo',['/pages/approve/approve']]]]]}}" class="list-item" bindtap="__e"><text class="list-num">{{datas[4]}}</text><text class="list">批准</text></view></view></view><view class="dashboard"><view class="dashboard1">工作台</view><view class="dashboard2"><view class="cu-capsule radius"><view class="cu-tag bg-grey ">工作量</view><view class="cu-tag line-grey">{{"今年:"+datas[5]+"，去年:"+datas[6]}}</view></view></view></view></view>